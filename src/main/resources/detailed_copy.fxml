<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DetailedController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="800.0" spacing="5.0">
         <children>
            <StackPane>
               <children>
                  <Pane fx:id="pane" />
                  <GridPane fx:id="gridPane" alignment="TOP_CENTER" prefHeight="310.0" prefWidth="800.0">
                     <children>
                        <Label fx:id="city" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                        <Label fx:id="currentTemp" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label fx:id="description" alignment="CENTER" prefHeight="60.0" prefWidth="400.0" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label fx:id="feelsLike" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <Label fx:id="highTemp" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <Label fx:id="lowTemp" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <Label fx:id="humidity" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="3" />
                        <Label fx:id="cloud" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                        <Label fx:id="windSpeed" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                        <Label fx:id="windGust" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                        <Label fx:id="windDirection" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" GridPane.columnIndex="3" GridPane.rowIndex="4" />
                        <Label fx:id="cityKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="City:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                        <Label fx:id="currentTempKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Current Temp:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                        <Label fx:id="descriptionKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Description:" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                        <Label fx:id="feelsLikeKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Feels Like:" GridPane.columnIndex="0" GridPane.rowIndex="3" />
                        <Label fx:id="highTempKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="High Temp:" GridPane.columnIndex="0" GridPane.rowIndex="4" />
                        <Label fx:id="lowTempKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Low Temp:" GridPane.columnIndex="0" GridPane.rowIndex="5" />
                        <Label fx:id="humidityKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Humidity:" GridPane.columnIndex="2" />
                        <Label fx:id="cloudKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Cloudiness:" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                        <Label fx:id="windSpeedKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Wind Speed:" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                        <Label fx:id="windGustKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Wind Gust:" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                        <Label fx:id="windDirectionKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Wind Direction:" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                     </children>
                     <columnConstraints>
                        <ColumnConstraints prefWidth="200.0" />
                        <ColumnConstraints prefWidth="200.0" />
                        <ColumnConstraints prefWidth="200.0" />
                        <ColumnConstraints prefWidth="200.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints />
                        <RowConstraints />
                        <RowConstraints />
                        <RowConstraints />
                        <RowConstraints />
                        <RowConstraints />
                     </rowConstraints>
                  </GridPane>
               </children>
            </StackPane>
            <HBox fx:id="hBox" alignment="CENTER" prefHeight="200.0" prefWidth="800.0" spacing="10">
               <children>
                  <VBox fx:id="vBoxKey" alignment="CENTER_LEFT" minWidth="100.0" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Day: " />
                        <Label text="High Temp:" />
                        <Label text="Low Temp: " />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Button fx:id="backButton" cancelButton="true" onAction="#backHandler" text="Back" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
