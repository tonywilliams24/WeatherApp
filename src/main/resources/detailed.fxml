<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DetailedController">
   <children>
      <GridPane fx:id="gridPane" prefHeight="600.0" prefWidth="400.0">
         <children>
            <Label fx:id="city" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Label" GridPane.columnIndex="1">
            </Label>
            <Label fx:id="temp" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="description" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="wind" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="cityKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="City:">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="tempKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Temperature:" GridPane.rowIndex="1">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="descriptionKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Description:" GridPane.rowIndex="2">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="windKey" alignment="CENTER" prefHeight="50.0" prefWidth="400.0" text="Wind:" GridPane.rowIndex="3">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
         </children>
      </GridPane>
   </children>
</AnchorPane>
